{
    "arrowParens": "always",
    "bracketSpacing": true,
    "bracketSameLine": true,
    "jsxSingleQuote": true,
    "printWidth": 120,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 4,
    "trailingComma": "none",
    "plugins": [
    ],
    "importOrder": [
        "^react$",
        "^next(/.*)?$",
        "next-themes",
        "@/*",
        "<THIRD_PARTY_MODULES>"
    ],
    "importOrderSeparation": true
}